{
  "name": "@js-toolkit/styled-components",
  "version": "1.2.0",
  "description": "Styled react components powered by JSS via @material-ui/styles",
  "author": "VZH",
  "license": "MIT",
  "main": "./index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/js-toolkit/styled-components"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json .npmignore README.md LICENSE ./dist/",
    "build-ts": "npm run clean && tsc --project ./tsconfig.json",
    "build": "npm run build-ts",
    "build-to-tutoron": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/@js-toolkit/styled-components'",
    "build-to-player": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/player/node_modules/@js-toolkit/styled-components'",
    "patch-publish": "yarn run build && yarn version --patch && yarn run copy:configs && npm publish ./dist/ && git push origin master --follow-tags",
    "minor-publish": "yarn run build && yarn version --minor && yarn run copy:configs && npm publish ./dist/ && git push origin master --follow-tags"
  },
  "devDependencies": {
    "@js-toolkit/configs": "^3.77.5",
    "@js-toolkit/react-hooks": "^1.33.0",
    "@js-toolkit/ts-utils": "^1.34.0",
    "@js-toolkit/web-utils": "^1.22.1",
    "@material-ui/styles": "^4.11.4",
    "@types/react": "17.0.11",
    "@types/react-modal": "^3.12.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jss": "^10.7.0",
    "jss-plugin-camel-case": "^10.7.0",
    "jss-plugin-compose": "^10.7.0",
    "jss-plugin-default-unit": "^10.7.0",
    "jss-plugin-expand": "^10.7.0",
    "jss-plugin-extend": "^10.7.0",
    "jss-plugin-global": "^10.7.0",
    "jss-plugin-nested": "^10.7.0",
    "jss-plugin-props-sort": "^10.7.0",
    "jss-plugin-rule-value-function": "^10.7.0",
    "jss-plugin-rule-value-observable": "^10.7.0",
    "jss-plugin-template": "^10.7.0",
    "jss-plugin-vendor-prefixer": "^10.7.0",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-modal": "^3.14.3",
    "react-outside-click-listener": "^1.3.2",
    "react-use": "^17.2.4",
    "reflexy": "^3.34.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "peerDependencies": {
    "react": ">= 16.8"
  }
}
